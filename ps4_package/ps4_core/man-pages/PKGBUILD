#Mantainer TigerClips1 <TigerClips1@ps4repo.site>
pkgname=man-pages
pkgver=6.06
pkgrel=1
pkgdesc=" The Man-pages package contains over 2,400 man pages."
arch=("any")
url="https://www.linuxfromscratch.org/lfs/downloads/stable-systemd/LFS-BOOK-12.1-systemd-NOCHUNKS.html#ch-system-man-pages"
license=("GPL3")
dpends=("coreutils")

source=("https://ftp.osuosl.org/pub/lfs/lfs-packages/12.1/man-pages-6.06.tar.xz")

sha256sums=("SKIP")

prepare() {
    rm -v man3/crypt*
}
build() {
    make
}

package() {
    make prefix=$pkgdir/usr install
}
