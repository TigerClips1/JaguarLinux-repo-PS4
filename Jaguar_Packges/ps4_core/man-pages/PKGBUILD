# Package Maintainers
MAINTAINERS=("TigerClips1 <tigerclips1@ps4repo.site>")

# Package information
NAME="man-pages"
VERSION="2.40.2"
UPSTREAM=1
DESC="The Man-pages package contains over 2,400 man pages."
GRPS=("core")
URL="https://man6.org"
LICENSES=("GPL")
DEPENDS=("ps4_core")

# Source information
SRC=("http://ftp.lfs-matrix.net/pub/lfs/lfs-packages/12.2/man-pages-6.9.1.tar.xz")


# Prepare script
function prepare() {
    cd "$WORKDIR/$NAME-$VERSION"
    rm -v man3/crypt*
    
    return 0
}

# Build script
function build() {
    return 0
}

function check() {
    return 0
}


# Post build script
function postbuild() {
   cd "$WORKDIR/$NAME-$VERSION"
   make  DESTDIR=${BUILD_DATA_ROOT} install

    return 0
}
