# Prepare script
function prepare() {
    return 0
}

# Build script
function build() {
    return 0
}

function check() {
    return 0
}


# Post build script
function postbuild() {
   cd "$WORKDIR/$NAME-$VERSION"
   mkdir -pv  ${BUILD_DATA_ROOT}/usr/etc
   cp services protocols ${BUILD_DATA_ROOT}/usr/etc
    return 0
}
